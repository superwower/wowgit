{
  "name": "wowgit",
  "version": "0.0.1",
  "description": "A git web client that wows you",
  "main": "server/index.ts",
  "scripts": {
    "dev": "ts-node-dev --ignore-watch .next server/index.ts",
    "build": "npm run build:clean && npm run build:next",
    "build:next": "next build",
    "build:clean": "rm -rf .next",
    "clean": "rm -rf node_modules .next",
    "start": "NODE_ENV=production ts-node server/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --list-different \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "format:fix": "prettier --write \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "lint": "npm run format && tslint ./{*,**/*}.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superwower/wowgit.git"
  },
  "keywords": [
    "git",
    "web"
  ],
  "author": "Super Wower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/superwower/wowgit/issues"
  },
  "homepage": "https://github.com/superwower/wowgit#readme",
  "dependencies": {
    "@types/next": "^6.0.4",
    "@zeit/next-typescript": "^1.1.0",
    "bulma": "^0.7.1",
    "fastify": "^1.7.0",
    "next": "^6.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "node-sass": "^4.9.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.13.7",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.3",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.10.0"
  }
}
