{
  "name": "wowgit",
  "version": "0.0.1",
  "description": "A git web client that wows you",
  "main": "server/index.ts",
  "scripts": {
    "dev": "ts-node-dev --ignore-watch .next server/index.ts",
    "build": "npm run clean && npm run build:next",
    "build:next": "next build",
    "clean": "rm -rf .next",
    "start": "NODE_ENV=production ts-node server/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --list-different \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "format:fix": "prettier --write \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "lint": "npm run format && tslint ./{*,**/*}.{ts,tsx}",
    "husky-hooks": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky-hooks",
      "pre-push": "npm run husky-hooks"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superwower/wowgit.git"
  },
  "keywords": [
    "git",
    "web"
  ],
  "author": "Super Wower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/superwower/wowgit/issues"
  },
  "homepage": "https://github.com/superwower/wowgit#readme",
  "dependencies": {
    "@types/next": "^6.0.4",
    "@zeit/next-typescript": "^1.1.0",
    "fastify": "^1.7.0",
    "next": "^6.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "typescript": "^2.9.2",
    "ts-node": "^7.0.0"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "prettier": "^1.13.7",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.10.0"
  }
}
